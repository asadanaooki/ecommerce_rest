<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.IdempotencyMapper">

  <insert id="insert">
    insert into idempotency (
      idempotency_key
    ) values (
      #{idempotencyKey}
    )
  </insert>
  
  <delete id="deleteExpired">
    delete from idempotency
    where updated_at &lt; date_sub(now(), interval 14 day)
  </delete>

  
</mapper>